; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "AudioPlayer",
"language": "GDScript",
"path": "res://Gameplay Scripts/audio_player.gd"
}, {
"base": "AnimationPlayer",
"class": "CharacterAnimator",
"language": "GDScript",
"path": "res://Gameplay Scripts/character_animator.gd"
}, {
"base": "Node2D",
"class": "PlayerCamera",
"language": "GDScript",
"path": "res://Gameplay Scripts/player_camera.gd"
}, {
"base": "KinematicBody2D",
"class": "PlayerPhysics",
"language": "GDScript",
"path": "res://Gameplay Scripts/player_physics.gd"
} ]
_global_script_class_icons={
"AudioPlayer": "",
"CharacterAnimator": "",
"PlayerCamera": "",
"PlayerPhysics": ""
}

[android]

2d/use_pixel_snap=false

[application]

config/name="Project GoDelta"
config/description="An engine extending and updating MarmitoTH work \"https://github.com/marmitoTH/Godot-Sonic-Physics\" to be feature be feature parity with Sonic Worlds Delta"
run/main_scene="res://main.tscn"
boot_splash/fullsize=false
config/icon="res://icon.png"

[display]

window/size/width=424
window/size/height=240
window/dpi/allow_hidpi=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[global]

Layer=false

[input]

ui_full_screen={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777244,"unicode":0,"echo":false,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="Defualt Platforms"
2d_physics/layer_2="Alternate Layer"

[physics]

2d/default_linear_damp=0.0

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
